{
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "https://origin.robnot.us.kg/:path*"
    }
  ],
  "headers": [
    {
      "source": "/:path*",
      "headers": [
        {
          "key": "X-Original-Host", 
          "value": "$host"
        },
        {
          "key": "X-Original-Path", 
          "value": "$url"
        }
      ]
    }
  ]
}
